{
  "name": "trms-back-end",
  "version": "1.0.0",
  "description": "Back-end for the Tuition Management System",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "dev": "nodemon --exec ts-node src/index.ts",
    "start:prod": "node build/index.js",
    "build": "(rm -r ./build || true) && tsc -outDir ./build",
    "build:start:prod": "npm run build && node build/index.js",
    "lint": "eslint **",
    "lint:fix": "eslint ** --fix",
    "db:create": "ts-node src/dynamo/table/createTable.ts",
    "db:delete": "ts-node src/dynamo/table/deleteTable.ts",
    "db:populate": "ts-node src/dynamo/table/populateData.ts",
    "db:reset": "npm run db:delete && sleep 30 && npm run db:create && sleep 30 && npm run db:populate"
  },
  "author": "DustinDiazLopez",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-fileupload": "^1.1.6",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/multer": "^1.4.6",
    "@types/node": "^15.6.1",
    "@types/nodemailer": "^6.4.2",
    "@types/shortid": "0.0.29",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "concurrently": "^6.2.0",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "randexp": "^0.5.3",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "aws-sdk": "^2.920.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-fileupload": "^1.2.1",
    "express-session": "^1.17.2",
    "http-status-codes": "^2.1.4",
    "log4js": "^6.3.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.2",
    "shortid": "^2.2.16",
    "uuid": "^8.3.2"
  }
}
